		create table urlist (
			id int(10) auto_increment not null primary key,
			url varchar(200) not null
		);
		
		create table wordlist (
			id int(10) auto_increment not null primary key,
			word varchar(30) not null
		);
				
		createString = "create table wordlist ("
				+ "id int(10) auto_increment not null primary key,"
				+ "url varchar(30) not null" 
				+ ")";	
				
		createString = "create table wordlocation ("
				+ "id int(20) auto_increment not null primary key,"
				+ "urlid int(10) not null,"
				+ "wordid int(10) not null,"
				+ "location int(10) not null" 
				+ ")";	
				
		select w0.urlid, w0.location, w1.location
		from 	wordlocation w0, wordlocation w1
		where w0.urlid=w1.urlid
		and w0.wordid=6745
		and w1.wordid=6746;
			